{
  "point": [
    {"action": "startmove"},
    {"action": "stopmove"}
  ],
  "click": [
    {"action": "mousedown"},
    {"action": "mouseup"}
  ],
  "drag": [
    {"action": "mousedown", "role": "draggable"},
    {"action": "point"},
    {"action": "mouseup", "role": "draggable"}
  ],
  "drag_drop": [
    {"action": "mousedown", "role": "draggable"},
    {"action": "point", "role": "drop_target"},
    {"action": "mouseup", "role": "draggable"}
  ],
  "press": [
    {"action": "keydown"},
    {"action": "keyup"}
  ],
  "hover": [
    {"action": "point"},
    {"action": "pause"}
  ],
  "click_carousel_nav": [
    {"action": "point", "element": "button", "role": "carousel-nav"},
    {"action": "click", "element": "button", "role": "carousel-nav", "repeatable": true},
    {"action": "point", "element": "a", "role": "carousel-link"},
    {"action": "click", "element": "a", "role": "carousel-link"}
  ],
  "scroll_carousel_container": [
    {"action": "point", "role": "carousel-container"},
    {"action": "scroll", "role": "carousel-container"}
  ],
  "click_tab": [
    {"action": "point", "element": "button", "role": "tab"},
    {"action": "click", "element": "button", "role": "tab"}
  ]
}